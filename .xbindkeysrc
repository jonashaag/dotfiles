# moc
"mocp --next"
  XF86AudioNext
"mocp --previous"
  Key XF86AudioPrev
"mocp --toggle-pause"
  Key XF86AudioPlay
"mocp --exit"
  Key XF86AudioStop

# special keys
"amixer -q set PCM 5%-"
  Mod4-F1
"amixer -q set PCM 5%-"
  XF86AudioLowerVolume
"amixer -q set PCM 5%+"
  Mod4-F2
"amixer -q set PCM 5%+"
  XF86AudioRaiseVolume
"amixer set Master toggle"
  Mod4-F3
"amixer set Master toggle"
  XF86AudioMute
"toggle_touchpad"
  XF86TouchpadToggle
"sudo pm-suspend &"
  XF86Sleep
